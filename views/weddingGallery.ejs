<div class="container">
	<h1 class="gallery-title">Wedding Images</h1>
	<a class="btn back-btn btn-lg btn-primary glyphicon glyphicon-arrow-left" href="/"></a>
	<div class="showImages">
		<ul class="gallery">
			<% weddingImages.forEach(function(image, idx){ %>
			<li><a href="wedding-images/<%= idx %>"><img class="gallery-img" src="<%= image.src %>"/></a></li>	
			<% }); %>
		</ul>
	</div>
</div>